<?xml version='1.0' encoding='utf-8'?>
<widget id="com.imie.tamago" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Tamago</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <icon density="mdpi" height="57" platform="ios" src="www/img/unnamed.png" width="57" />
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <preference name="Fullscreen" value="true" />
        <allow-intent href="market:*" />
        <splash density="land-hdpi" src="www/img/oeuf.jpg" />
        <splash density="land-ldpi" src="www/img/oeuf.jpg" />
        <splash density="land-mdpi" src="www/img/oeuf.jpg" />
        <splash density="land-xhdpi" src="www/img/oeuf.jpg" />
        <splash density="port-hdpi" src="www/img/oeuf.jpg" />
        <splash density="port-ldpi" src="www/img/oeuf.jpg" />
        <splash density="port-mdpi" src="www/img/oeuf.jpg" />
        <splash density="port-xhdpi" src="www/img/oeuf.jpg" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <engine name="android" spec="^6.2.3" />
    <engine name="browser" spec="^4.1.0" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.2" />
    <plugin name="cordova-plugin-splashscreen" spec="^4.0.3" />
    <plugin name="cordova-plugin-fullscreen" spec="~1.1.0" />
</widget>
